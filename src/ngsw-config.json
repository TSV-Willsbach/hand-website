{
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html"
            ],
            "versionedFiles": [
                "/*.bundle.css",
                "/*.bundle.js",
                "/*.chunk.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**"
            ]
        }
    }],
    "dataGroups": [{
        "name": "api-performance",
        "urls": [
            "/",
            "/postDetail",
            "/sponsors",
            "/reports"
        ],
        "cacheConfig": {
            "maxSize": 100,
            "maxAge": "1d",
            "strategy": "performance"
        }
    }]
}