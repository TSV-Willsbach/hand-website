{
    "configVersion": 1,
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html"
            ],
            "versionedFiles": [
                "/*.bundle.css",
                "/*.bundle.js",
                "/*.chunk.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**"
            ]
        }
    }],
    "dataGroups": [{
            "name": "api-performance",
            "urls": [
                "/postDetail",
                "/sponsors",
                "/reports",
                "/team/*"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1d",
                "strategy": "performance"
            }
        },
        {
            "name": "api-freshness",
            "urls": [
                "/"
            ],
            "cacheConfig": {
                "maxSize": 30,
                "timeout": "5s",
                "maxAge": "1h",
                "strategy": "freshness"
            }
        }
    ]
}