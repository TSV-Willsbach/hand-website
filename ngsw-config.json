{
    "configVersion": 7,
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/*.css",
                "/*.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**"
            ]
        }
    }],
    "dataGroups": [{
            "name": "api-performance",
            "urls": [
                "/",
                "/postDetail/*",
                "/sponsors",
                "/reports",
                "/team/*"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1d",
                "strategy": "performance"
            }
        },
        {
            "name": "api-freshness",
            "urls": [
                "https://api.willsbach-handball.de/*"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "timeout": "10s",
                "maxAge": "1h",
                "strategy": "freshness"
            }
        }
    ]
}